{
  "name": "@andresandoval/commons-js",
  "version": "0.0.2",
  "description": "Common stuff for ts/js projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "postbuild": "ts-node postbuild.js",
    "build:watch": "tsc-watch --onSuccess 'npm run postbuild'",
    "test": "karma start ./karma.config.js",
    "pretest": "rimraf spec && rimraf coverage && mkdir -p spec",
    "test:single": "ts-node node_modules/jasmine/bin/jasmine.js --config=jasmine.config.json",
    "preprebuild:prod": "npm run lint",
    "prebuild:prod": "npm run test",
    "build:prod": "npm run build"
  },
  "author": "Andres Sandoval <https://github.com/andresandoval>",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "jasmine-core": "^5.2.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-webpack": "^5.0.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.3.0"
  },
  "keywords": []
}
